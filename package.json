{
	"name": "tukal-vue",
	"version": "0.1.114",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"serve": "vite preview",
		"test": "vitest --run --environment jsdom",
		"serve:docs": "vitepress dev",
		"build:docs": "vitepress build docs",
		"generate-docs": "node generate-docs.js"
	},
	"main": "dist/tukal-vue.umd.cjs",
