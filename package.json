		"@tiptap/extension-underline": "^2.0.0-beta.204",
		"@tiptap/starter-kit": "^2.0.0-beta.202",
		"@tiptap/vue-3": "^2.0.0-beta.202",
		"@vue/test-utils": "^2.3.2",
		"http-server": "^14.1.1",
		"jsdom": "^21.1.1",
		"jspdf": "^2.5.1",
		"jspdf-autotable": "^3.5.25",
		"lodash": "^4.17.21",
		"mustache": "^4.2.0",
